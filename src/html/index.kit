<!DOCTYPE html>
<html lang="en" class="js">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <link rel="icon" href="../assets/favicon/favicon-64.png" />
    <link
      rel="apple-touch-icon"
      href="../assets/favicon/apple-touch-icon-128.png"
    />
    <link rel="manifest" href="../assets/favicon/manifest.webmanifest" />

    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Work+Sans:wght@300;400;500;600;700&display=swap"
      rel="stylesheet"
    />

    <title>8X EQuity Holding</title>

    <link rel="stylesheet" href="./css/main.min.css" />
    <script defer src="./js/index.min.js"></script>
  </head>
  <body>
    <header class="header js-header">
      <div class="wrapper">
        <!-- @import "nav.kit"-->
      </div>
    </header>

    <main>
      <section class="hero" id="hero" data-section="hero">
        <div class="hero__swiper swiper">
          <div class="hero__swiper-wrapper swiper-wrapper">
            <!-- Slide 1 -->
            <div class="hero__swiper-slide swiper-slide">
              <div
                class="hero__background"
                style="
                  background-image: url(../assets/desktop/Home/bgHeader.png);
                "
              >
                <div class="wrapper">
                  <div class="hero__content orange-line rtx">
                    <h1>
                      <strong>Private Equity</strong> for Innovative Hardwares
                      and Technologies Companies
                    </h1>
                  </div>
                  <a href="javascript:" class="hero__btn btn"
                    ><span class="hero__btn-text">Learn More</span
                    ><span class="hero__btn-icon icon-arrow_forward"></span
                  ></a>
                </div>
              </div>
            </div>
            <!-- Slide 2 -->
            <div class="hero__swiper-slide swiper-slide">
              <div
                class="hero__background"
                style="
                  background-image: url(../assets/desktop/Home/bgHeader.png);
                "
              >
                <div class="wrapper">
                  <div class="hero__content orange-line rtx">
                    <h1>
                      <strong>Privateeeee Equity</strong> for Innovative
                      Hardwares and Technologies Companies
                    </h1>
                  </div>
                  <a href="javascript:" class="hero__btn btn"
                    ><span class="hero__btn-text">Learn More</span
                    ><span class="hero__btn-icon icon-arrow_forward"></span
                  ></a>
                </div>
              </div>
            </div>
          </div>
          <div class="swiper-pagination"></div>
        </div>
        <a href="#investment" class="hero__btn-scroll"></a>
      </section>

      <section
        class="investment"
        id="investment"
        style="background-image: url(../assets/desktop/Home/bgVision.png)"
        data-section="investment"
      >
        <div class="investment__wrapper wrapper">
          <div class="investment__content orange-line rtx">
            <h2>Investment <span>Vision</span></h2>
            <p>
              We are continuosly seeking for companies at venture capital or
              Series A stage with energic, driven and innovative founders.
            </p>
          </div>
        </div>
      </section>
    </main>
    <!-- @import "footer.kit"-->
    <div class="overlay js-overlay"></div>
  </body>
</html>
